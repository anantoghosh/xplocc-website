{
  "name": "xplocc-www",
  "description": "Xplocc Website",
  "version": "1.0.0",
  "author": "Ananto Ghosh",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "gatsby": "^2.0.3",
    "gatsby-image": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-netlify-cms": "^3.0.18",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-purgecss": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.3",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-sharp": "^2.1.1",
    "netlify-cms": "^2.9.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-obfuscate": "^3.6.2",
    "size-plugin": "^1.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint-fix": "eslint src/**/*.{js,jsx} --fix",
    "typecheck": "tsc -p jsconfig.json",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy": "yarn build && netlify deploy"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.7",
    "@types/webpack": "^4.4.11",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-problems": "^1.1.0",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-transformer-remark": "^2.1.3",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "plop": "^2.0.0",
    "prettier": "^1.14.2",
    "typed-css-modules-loader": "^0.0.14",
    "typescript": "^3.0.3",
    "webpack-bundle-size-analyzer": "^2.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
