{
  "name": "xplocc-www",
  "description": "Xplocc Website",
  "version": "1.0.0",
  "author": "Ananto Ghosh",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "gatsby": "^2.19.17",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-netlify": "^2.1.32",
    "gatsby-plugin-netlify-cms": "^4.1.40",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.3.14",
    "netlify-cms-app": "^2.11.19",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-obfuscate": "^3.6.6",
    "size-plugin": "^1.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint-fix": "eslint src/**/*.{js,jsx} --fix",
    "typecheck": "tsc -p jsconfig.json",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy": "yarn build && netlify deploy",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@types/webpack": "^4.41.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "typed-css-modules-loader": "^0.0.18",
    "typescript": "^3.8.2",
    "webpack-bundle-size-analyzer": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,md,json,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
