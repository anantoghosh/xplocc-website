{
  "name": "xplocc-www",
  "description": "Xplocc Website",
  "version": "1.0.0",
  "author": "Ananto Ghosh",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "gatsby": "^2.9.7",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-netlify-cms": "^3.0.18",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.1.7",
    "gatsby-remark-images": "^3.0.16",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.43",
    "gatsby-transformer-remark": "^2.4.0",
    "gatsby-transformer-sharp": "^2.1.21",
    "netlify-cms": "^2.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-obfuscate": "^3.6.2",
    "size-plugin": "^1.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint-fix": "eslint src/**/*.{js,jsx} --fix",
    "typecheck": "tsc -p jsconfig.json",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy": "yarn build && netlify deploy",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
    "@types/webpack": "^4.4.33",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-problems": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "plop": "^2.3.0",
    "prettier": "^1.18.2",
    "typed-css-modules-loader": "^0.0.17",
    "typescript": "^3.5.2",
    "webpack-bundle-size-analyzer": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,md,json,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
